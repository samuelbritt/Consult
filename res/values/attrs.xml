<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CalculatorItem">
        <attr name="labelText" format="string" localization="suggested" />
        <attr name="labelAllCaps" format="boolean" />
        <attr name="labelVisibility">
            <enum name="visible" value="1" />
            <enum name="invisible" value="2" />
            <enum name="gone" value="3" />
        </attr>
        <attr name="unitText" format="string" localization="suggested" />
    </declare-styleable>

</resources>